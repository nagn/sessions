<div id = "sidebuttons">
  <%= button_to "Download CSV", session_path(@session, :format => "csv"), :method => :get  %>
  <%= button_to "Delete This Session", {:controller => :sessions, :action => 'destroy', :id => @session.id }, :method => :delete, data: {:confirm => 'Are you sure you would like to delete this session?'}, :id => "delete_session" %>
</div>
<div id = "title">
  <h1><%=@session.title %></h1>
</div>
<div id = "subtitle">
  <%= @session.date%>
  <%= @session.kind%> 
</div>
<div id = "description">
  <%= @session.description%>
</div>
<div id = "students">
  <%= render "attending_students" %>
</div>
<%= render "selected_users"%>
<%= form_for(@session) do |f| %>
  <p>
    <%= f.submit 'Submit' %>
  </p>
<% end %>
<%= link_to "<< Back to Sessions List", sessions_path %>