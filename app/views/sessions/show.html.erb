<h1><%= @session.title %></h1>
<p><%= @session.description %></p>
<p><%= @session.date%></p>
<p><%= @session.kind%></p>
<%= link_to "<< Back to Sessions List", sessions_path %>
<div id = "students">
  <%= render "attending_students" %>
</div>
<%= render "selected_users"%>
<%= form_for(@session) do |f| %>
  <p>
    <%= f.submit 'Submit' %>
  </p>
<% end %>